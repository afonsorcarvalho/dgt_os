<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_os" model="ir.sequence">
            <field name="name">Ordem de servico</field>
            <field name="code">dgt_os.os</field>
            <field name="prefix">OS</field> 
            <field name="prefix">OS%(range_y)s%(range_month)s.</field>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
		 <record id="seq_os_relatorio" model="ir.sequence">
            <field name="name">Relatório de servico</field>
            <field name="code">dgt_os.os.relatorio</field>
			<field name="prefix">RAT/%(range_year)s/</field>
			<field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
		<record id="seq_os_request" model="ir.sequence">
            <field name="name">Solicitacao de servico</field>
            <field name="code">dgt_os.os.request</field>
			<field name="prefix">SS/%(range_year)s/</field>
			<field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
		<record id="seq_os_plantao_viagem" model="ir.sequence">
            <field name="name">Plantões e Viagens</field>
            <field name="code">dgt_os.os.plantao.viagem</field>
			<field name="prefix">RDV/%(range_year)s/</field>
			<field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
